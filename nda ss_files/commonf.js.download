//function to check the numberic value
function checkNum(cspnoofatt){
   var s_len=cspnoofatt.length ;
   var s_charcode = 0;
    for (var s_i=0;s_i < s_len;s_i++){
     s_charcode = cspnoofatt.charCodeAt(s_i);
     if(!((s_charcode>=48 && s_charcode<=57))){
        return 1;
      }
    }
}
