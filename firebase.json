{
  "hosting": {
    "public": "public",
    "site": "tajtourandtrv",
    "cleanUrls": true,
    "trailingSlash": false,
    "redirects": [
      {
        "source": "/nda-ss",
        "destination": "/",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**/*.@(js|css|gif|jpg|jpeg|png|svg|webp|webm|eot|otf|ttf|woff|woff2)",
        "destination": "/index.html"
      },
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/contact",
        "destination": "/contact.html"
      },
      {
        "source": "/booking",
        "destination": "/booking.html"
      },
      {
        "source": "/destination",
        "destination": "/destination.html"
      },
      {
        "source": "/service",
        "destination": "/service.html"
      },
      {
        "source": "/team",
        "destination": "/team.html"
      },
      {
        "source": "/testimonial",
        "destination": "/testimonial.html"
      },
      {
        "source": "/package",
        "destination": "/package.html"
      },
      {
        "source": "/about",
        "destination": "/about.html"
      },
      {
        "source": "/faqsandhelp",
        "destination": "/faqsandhelp.html"
      },
      {
        "source": "/privacy-and-policy",
        "destination": "/privacyandpolicy.html"
      },
      {
        "source": "/terms-and-conditions",
        "destination": "/termsandconditions.html"
      },
      {
        "source": "/golden-triangle-tour",
        "destination": "/pkg/golden_triangle.html"
      },
      {
        "source": "/delhi-vrindavan-package",
        "destination": "/pkg/delhitovrindavan.html"
      },
      {
        "source": "/delhi-mathura-package",
        "destination": "/pkg/delhitomathura.html"
      },
      {
        "source": "/delhi-agra-jaipur-package",
        "destination": "/pkg/delhiagrajaipur.html"
      },
      {
        "source": "/same-day-tajmahal-tour",
        "destination": "/pkg/sameday_tajmahaltour.html"
      },
      {
        "source": "/tajmahal-sunset-tour",
        "destination": "/pkg/tajmahal_sunsettour.html"
      },
      {
        "source": "/ayodhya-package",
        "destination": "/pkg/ayodhya_package.html"
      },
      {
        "source": "/ayodhya-tour",
        "destination": "/pkg/ayodhya_package.html"
      },
      {
        "source": "/kumbh-package",
        "destination": "/pkg/kumbh_package.html"
      },
      {
        "source": "/holi-special-package",
        "destination": "/pkg/holi-special-package.html"
      },
      {
        "source": "/taj-mahal",
        "destination": "/blogs/tajmahal.html"
      },
      {
        "source": "/delhiagra",
        "destination": "/pkg/delhiagra.html"
      },
      {
        "source": "/mathura-tour",
        "destination": "/pkg/mathura-tour.html"
      },
      {
        "source": "/jaipur-tour",
        "destination": "/pkg/jaipur-tour.html"
      },
      {
        "source": "/two-days-mathura-vrindavan-tour",
        "destination": "/pkg/twodaysmathuravrindavan.html"
      },
      {
        "source": "/same-day-agra-tour",
        "destination": "/pkg/samedayagra.html"
      },
      {
        "source": "/same-day-jaipur-tour",
        "destination": "/pkg/samedayjaipur.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.@(js|css)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      },
      {
        "source": "**/*.@(jpg|jpeg|gif|png|webp|svg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          }
        ]
      },
      {
        "source": "**/*.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "SAMEORIGIN"
          }
        ]
      },
      {
        "source": "/robots.txt",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=86400"
          }
        ]
      },
      {
        "source": "/sitemap.xml",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=86400"
          }
        ]
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "design.css.bak",
      "nda ss.html",
      "nda ss_files/**",
      "publicd/**"
    ]
  }
}
